APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Internal
DB_CONNECTION=mysql
DB_HOST=mysql_internal
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=laravel_user
DB_PASSWORD=secret
DB_ROOT_PASSWORD=root_secret

# Database External 1
DB_HOST_SECONDARY=external_db1_host
DB_PORT_SECONDARY=3306
DB_DATABASE_SECONDARY=external_db1
DB_USERNAME_SECONDARY=external_user1
DB_PASSWORD_SECONDARY=external_pass1

# Database External 2
DB_HOST_TERTIARY=external_db2_host
DB_PORT_TERTIARY=3306
DB_DATABASE_TERTIARY=external_db2
DB_USERNAME_TERTIARY=external_user2
DB_PASSWORD_TERTIARY=external_pass2

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=null
